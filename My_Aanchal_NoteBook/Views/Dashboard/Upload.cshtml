
@{
    ViewData["Title"] = "Upload";
    Layout = "~/Views/Shared/_Master.cshtml";
}

<h1>Upload</h1>

<form method="post" asp-controller="Dashboard" asp-action="Upload" enctype="multipart/form-data">
    <div class="form-group">
        @Html.Label("file", "Upload Image:", new { @class = "form-label" })
        @Html.TextBox("file", null, new { type = "file", @class = "form-control", required = "required" })
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Upload</button>
    </div>
</form>



<script src="~/lib/jquery/dist/jquery.min.js"></script>
